{
  "name": "mindulbot",
  "version": "1.1.0",
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec ts-node bot.ts",
    "prepublish": "npm run pretty",
    "pretty": "prettier --config .prettierrc --write bot.ts src/**/*.ts"
  },
  "dependencies": {
    "@discordjs/opus": "^0.5.3",
    "@discordjs/voice": "^0.9.0",
    "discord.js": "^13.7.0",
    "dotenv": "^16.0.1",
    "libsodium-wrappers": "^0.7.10",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "nodemon": "^1.7.1",
    "play-dl": "^1.9.4",
    "prism-media": "1.3.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.2",
    "v8": "^0.1.0"
  },
  "devDependencies": {
    "prettier": "^2.6.2"
  }
}
