    "nodemon": "^2.0.22",
    "openai": "^3.2.1",
    "play-dl": "^1.9.6",
    "prism-media": "^1.3.5",
    "ts-node": "^10.9.1",
    "v8": "^0.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "eslint": "^8.40.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4"
